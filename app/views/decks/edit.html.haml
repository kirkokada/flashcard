- provide(:title, @deck.title)
%h1 Edit Deck
.row
  %aside.span3
    = form_for @deck do |f|
      = render 'form', f:f
      = f.submit 'Save Changes', class:"btn btn-large btn-primary"
  .span9
    - if @deck.cards.any?
      #card-accordion.accordion
        = render @cards
      = will_paginate @cards
    = link_to "New card", new_card_path, class: "btn btn-large btn-primary"
